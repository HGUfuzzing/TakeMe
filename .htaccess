RewriteEngine On

RewriteRule ^$ mainpage.php

RewriteBase /
Options -Indexes

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^p/mainpage$ mainpage.php
RewriteRule ^p/write$ create_post.php
RewriteRule ^p/edit/([가-힣a-zA-Z0-9-]+)$ update_post.php?keyword=$1


RewriteRule ^([가-힣a-zA-Z0-9-]+)$ read_post.php?keyword=$1

